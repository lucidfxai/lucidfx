{
  "name": "lucidfx",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.16.1"
  },
  "scripts": {
    "dev": "next dev",
    "ngrok": "ts-node -P tsconfig.scripts.json ./scripts/ngrok.ts && ngrok http 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docker:up": "cd docker && docker compose up -d && cd ..",
    "docker:down": "cd docker && docker compose down && cd ..",
    "generate:schema": "pnpm drizzle-kit generate:mysql --schema=./src/db/schema/*",
    "migrate": "ts-node -P tsconfig.scripts.json ./scripts/migrate.ts",
    "test:unit": "jest --config jest.unit.config.js",
    "test:integration": "jest --config jest.integration.config.js",
    "test": "pnpm test:unit && pnpm test:integration"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.10.15",
    "@clerk/nextjs": "^4.21.13",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.32.0",
    "@trpc/next": "^10.32.0",
    "@trpc/server": "^10.32.0",
    "@types/node": "20.3.1",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "autoprefixer": "10.4.14",
    "aws-sdk": "^2.1411.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.27.0",
    "drizzle-zod": "^0.4.4",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.7",
    "mysql2": "^3.4.2",
    "next": "13.4.7",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "superjson": "^1.12.4",
    "svix": "^1.5.2",
    "tailwindcss": "3.3.2",
    "url": "^0.11.1",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
    "@types/react-dom": "^18.2.4",
    "@types/supertest": "^2.0.12",
    "drizzle-kit": "^0.19.2",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "ngrok": "5.0.0-beta.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  }
}
